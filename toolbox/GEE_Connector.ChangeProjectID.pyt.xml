  <tool xmlns="" name="ChangeProjectID" displayname="Check or Change Project ID" toolboxalias="toolbox">
    <arcToolboxHelpPath>c:\program files\arcgis\pro\Resources\Help\gp</arcToolboxHelpPath>
    <parameters>
      <param name="current_id" displayname="Current project ID is shown below" type="Optional" direction="Input" datatype="String" expression="{current_id}">
        <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Current project ID automatically displays. No need for user input. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
      </param>
      <param name="current_tag" displayname="Current workload tag is shown below" type="Optional" direction="Input" datatype="String" expression="{current_tag}">
        <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Current workload tag automatically displays. No need for user input.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
      </param>
      <param name="new_id" displayname="Specify the new project ID" type="Required" direction="Input" datatype="String" expression="new_id">
        <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Input the new project ID here. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
      </param>
      <param name="new_tag" displayname="Specify the new workload tag" type="Optional" direction="Input" datatype="String" expression="{new_tag}">
        <dialogReference>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Input the new workload tag if any.&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</dialogReference>
      </param>
    </parameters>
    <summary>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;This script displays the current Google Cloud quota project ID and workload tag, and allows users to switch quota project ID and workload tag associated with Google Earth Engine.&lt;/SPAN&gt;&lt;/P&gt;&lt;P&gt;&lt;SPAN&gt;Please note that if you have installed Google Cloud SDK shell, you cannot switch quota project ID using this script. Instead, open the Google Cloud SDK shell, copy and paste the following command `gcloud auth application-default set-quota-project QUOTA_PROJECT_ID`, replace the `QUOTA_PROJECT_ID` with your target project ID. &lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;&lt;/DIV&gt;</summary>
  </tool>
